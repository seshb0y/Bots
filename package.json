{
  "name": "discordbot",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev src/index.ts",
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "build:prod": "tsc && npm run copy-files",
    "copy-files": "cp package.json dist/ && cp .env dist/ && cp -r src/data dist/",
    "start:prod": "node dist/index.js",
    "register": "ts-node src/register-commands.ts",
    "test": "ts-node run-tests.ts",
    "test:build": "npm run build && npm run test",
    "start:safe": "npm run test && npm run start",
    "git:add": "git add .",
    "git:commit": "git commit -m \"Update: automated commit\"",
    "git:push": "git push",
    "push": "npm run git:add && npm run git:commit && npm run git:push",
    "push:msg": "git add . && git commit && git push",
    "watch:git": "ts-node git-watcher.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@discordjs/rest": "^2.5.0",
    "@discordjs/voice": "^0.18.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@types/wav": "^1.0.4",
    "axios": "^1.9.0",
    "cheerio": "^1.1.0",
    "discord-api-types": "^0.38.11",
    "discord.js": "^14.19.3",
    "dotenv": "^16.5.0",
    "node-fetch": "^3.3.2",
    "node-record-lpcm16": "^1.0.1",
    "openai": "^5.12.2",
    "pidusage": "^4.0.1",
    "prism-media": "^1.3.5",
    "puppeteer": "^24.16.0",
    "undici": "^7.13.0",
    "wav": "^1.0.2"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "@types/pidusage": "^2.0.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "description": ""
}
